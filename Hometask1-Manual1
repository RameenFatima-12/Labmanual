#include <iostream>

using namespace std;
//hometask 1
int main() {
   int x1,x2,y1,y2,d,dy,dx;
   //taking user input for co-ordinates
   cout << "Enter the x co-ordinate of the first point" << endl;
   cin >> x1;
   cout << "Enter the y co-ordinate of the first point" << endl;
   cin >> y1;
   cout << "Enter the x co-ordinate of the second point" << endl;
   cin >> x2;
   cout << "Enter the y co-ordinate of the second point" << endl;
   cin >> y2;
   dx = x2 - x1;//the difference between x co-ordinates
   dy = y2 - y1;//the difference between y co-ordinates
   d = (dx*dx) + (dy*dy);//using the distance formula from lab manual
   cout << "The distance betweeen points is:" << d;
   return 0;
}
